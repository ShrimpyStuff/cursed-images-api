{
	"compilerOptions": {
		"target": "es5",
		"module": "CommonJS",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"noImplicitAny": true,
		"allowJs": true,
		"noImplicitThis": true,
		"outDir": "./dist"
	},
	"include": ["src"]
}
